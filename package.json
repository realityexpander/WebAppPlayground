{
  "name": "javascriptwebcomponents",
  "version": "1.0.0",
  "description": "web components app testing ground",
  "source": "src/app.js",
  "module": "dist/app.js",
  "type": "module",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.17.0",
    "parcel": "latest",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc-dev": "tsc --watch --preserveWatchOutput",
    "css-to-js": "./build-tools/convertCssToJsForWebComponents.bash",
    "rollup-build": "npm run css-to-js && rm -f dist/*.* && rollup -c --sourcemap",
    "rollup-force-no-stop": "while :\n do\n rollup -c -w --sourcemap\n done\n",
    "wds-noderes": "wds --watch --app-index index.html --node-resolve --open",
    "wds-esbuild": "wds --watch --app-index index.html --esbuild-target auto",
    "start-dev-rollup-force-watch": "watch -d -n1 -t -c rollup -c -w --sourcemap",
    "serve-ngrok": "ngrok http http://localhost:8081",
    "serve-ngrok-oauth": "ngrok http http://localhost:8081 --oauth=google --oauth-allow-email=realityexpander@gmail.com",
    "serve-ngrok-basicauth": "ngrok http http://localhost:8081 --basic-auth user:password",
    "vite-dev": "vite",
    "cypress:run": "cypress run --headless --browser chrome",
    "cypress:open": "cypress open",
    "cypress:local-run": "npx cypress run --record --key 3fb68fff-d21a-4abe-817a-ef31d8303087",
    "ci": "npm run rollup-build && wds --node-resolve --app-index index.html --esbuild-target auto --port 8081",
    "start-dev-tsc-wds-1": "concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds --open --watch --app-index index.html\"",
    "start-dev-watch-rollup": "npm run rollup-build && rollup -c -w --sourcemap"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lit-labs/context": "^0.3.3",
    "@lit-labs/task": "^3.0.0",
    "@lit/reactive-element": "^1.6.1",
    "@material/data-table": "^14.0.0",
    "@material/drawer": "^14.0.0",
    "@material/menu": "^14.0.0",
    "@material/top-app-bar": "^14.0.0",
    "@material/web": "^1.0.0-pre.5",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/helpers": "^0.4.14",
    "@types/uuid": "^9.0.1",
    "@web/dev-server": "^0.1.36",
    "@web/dev-server-esbuild": "^0.3.5",
    "concurrently": "^8.0.1",
    "cypress": "^12.9.0",
    "lit": "^2.7.0",
    "lit-element": "^3.3.3",
    "lit-html": "^2.8.0",
    "rollup": "^2.79.1",
    "simple-wc-router": "^2.3.4",
    "smart-webcomponents": "^15.2.10",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "vite": "^4.2.1"
  }
}
